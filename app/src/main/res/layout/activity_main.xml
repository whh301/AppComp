<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.spirent.landslide.datagen.MainActivity"
    android:fadeScrollbars="false"
    android:scrollbarStyle="insideOverlay"
    android:scrollbars="vertical"
    android:background="@drawable/bg"
    android:verticalScrollbarPosition="right">

    <TextView
        android:text="Dummy Carrier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtCarrierInfo"
        android:layout_alignBaseline="@+id/lblCarrier"
        android:layout_alignBottom="@+id/lblCarrier"
        android:layout_centerHorizontal="true" />

    <TextView
        android:text="China"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView14"
        android:layout_alignLeft="@+id/txtCarrierInfo"
        android:layout_alignStart="@+id/txtCarrierInfo"
        android:id="@+id/lblLocalInfo" />

    <TextView
        android:text="CDMA"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView"
        android:layout_alignLeft="@+id/lblLocalInfo"
        android:layout_alignStart="@+id/lblLocalInfo"
        android:id="@+id/txtNetworkInfo" />

    <TextView
        android:text="Network Info:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView"
        android:layout_below="@+id/textView14"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14" />

    <TextView
        android:text="Location:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView14"
        android:layout_below="@+id/lblCarrier"
        android:layout_alignLeft="@+id/lblCarrier"
        android:layout_alignStart="@+id/lblCarrier" />

    <TextView
        android:text="ON"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/txtWiFiInfo"
        android:layout_alignLeft="@+id/lblGsmSignal"
        android:layout_alignStart="@+id/lblGsmSignal"
        android:id="@+id/lblWiFiState" />

    <TextView
        android:text="-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblGsmSignal"
        android:layout_alignLeft="@+id/lblCdmaSignal"
        android:layout_alignStart="@+id/lblCdmaSignal"
        android:layout_below="@+id/lblCdmaSignal" />

    <TextView
        android:text="-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/txtNetworkInfo"
        android:layout_alignLeft="@+id/lblCdmaSignal"
        android:layout_alignStart="@+id/lblCdmaSignal"
        android:id="@+id/lblCellId" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Label_WifiDetail"
        android:layout_below="@+id/txtWiFiInfo"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView" />

    <TextView
        android:text="GSM Signal:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView6"
        android:layout_alignLeft="@+id/textView4"
        android:layout_alignStart="@+id/textView4"
        android:layout_below="@id/textView4" />

    <TextView
        android:text="Celular Information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="-1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblCdmaSignal"
        android:layout_below="@id/lblCellId"
        android:layout_alignLeft="@+id/txtNetworkInfo"
        android:layout_alignStart="@+id/txtNetworkInfo" />

    <TextView
        android:text="Celular Information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtCelular"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="WiFi Information"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtWiFiInfo"
        android:layout_below="@+id/textDataInfo"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:text="Cell Id:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView10"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/textView14"
        android:layout_alignStart="@+id/textView14" />

    <TextView
        android:text="CDMA Signal:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView4"
        android:layout_below="@+id/textView10"
        android:layout_alignRight="@+id/textView"
        android:layout_alignEnd="@+id/textView" />

    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textDataInfo"
        android:layout_below="@id/textView6"
        android:layout_alignLeft="@+id/textView6"
        android:layout_alignStart="@+id/textView6" />

    <TextView
        android:text="WiFi List"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/txtWiFiList"
        android:layout_below="@+id/Label_WifiDetail"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
    android:text="Carrier Info:"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
        android:id="@+id/lblCarrier"
        android:layout_below="@+id/txtCelular"
        android:layout_alignRight="@+id/txtWiFiInfo"
        android:layout_alignEnd="@+id/txtWiFiInfo" />


</RelativeLayout>
